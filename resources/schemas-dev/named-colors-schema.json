{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"additionalProperties": false,
	"required": [
		"named_colors"
	],
	"properties": {
		"named_colors": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"required": [
					"id",
					"color"
				],
				"type": "object",
				"properties": {
					"id": {
						"type": "string"
					},
					"color": {
						"type": "string"
					}
				}
			}
		}
	}
}